<%- include('../partials/header') %>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel='stylesheet' href='/stylesheets/style.css' />

    <title>HomePage</title>
</head>
<body>
 
    <h1>Welcome to StyLeKeebMods!</h1>

    
   <!-- Slideshow container -->
   <% if (user) { %>
    <a href="/keyboards/<%= user._id %>">Click here to get started!</a>
    <% } %>

    <br><br>
    
    <iframe style="border: 0; width:100%; height: 500px; overflow: auto;" src="https://www.youtube.com/embed/usNx1_d0HbQ">
    </iframe>
      
    <br>
    <br>
<table id="list">
<thead>
  <tr>
    <th>All Keyboard Builds</th>

  </tr>
</thead>
    <tbody>
    <% keyboards.forEach(function(k) { %>
        <tr>
            
            <td><%= k.name %></td>
            <td><a href="/keyboards/<%= k._id %>/details">Details</a></td>
              
            

          </tr>
    <% }); %>

</tbody>
</table>
</body>
</html>

<%- include('../partials/footer') %>